  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend:admin\" \"npm run frontend\"",
    "backend": "nodemon --watch backend --ext ts,json --exec npx ts-node -P tsconfig.node.json backend/server.ts",
    "frontend": "vite --config vite.public.config.ts",
    "frontend:admin": "vite --config vite.admin.config.ts",
    "build:public": "vite build --config vite.public.config.ts",
    "build:admin": "vite build --config vite.admin.config.ts",
    "build": "tsc -b && npm run build:public && npm run build:admin",
    "lint": "eslint .",
    "preview": "vite preview",
    "_comment": "para produccion -- start: node ./dist/server.js",
